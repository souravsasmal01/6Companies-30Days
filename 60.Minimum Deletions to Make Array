class Solution:
    def minOperations(self, nums: List[int], numsDivide: List[int]) -> int:
        def GCDofArray(a):
            b = a[0]
            for j in range(1,len(a)):
                s = math.gcd(b,a[j])
                b=s
            return b
        val = GCDofArray(numsDivide)
        heapq.heapify(nums)
        res = 0
        while len(nums) > 0 and val % nums[0]:
            res +=1
            heapq.heappop(nums)
        if len(nums) > 0:
            return res
        else:
            return -1
