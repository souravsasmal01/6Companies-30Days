class Solution:
    def isSameTree(self, p: Optional[TreeNode], q: Optional[TreeNode]) -> bool:
        def dfs(p,q):
            if not p and not q:
                return True
            elif (p and not q) or (q and not p) or p.val !=q.val:
                return False
            return dfs(p.left,q.left) and dfs(p.right, q.right)
        return dfs(p,q)
